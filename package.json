{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "remix build",
    "dev:remix": "remix watch",
    "dev:wrangler": "cross-env NODE_ENV=development npm run wrangler",
    "dev": "npm-run-all build --parallel \"dev:*\"",
    "start": "cross-env NODE_ENV=production npm run wrangler",
    "wrangler": "wrangler pages dev ./public --compatibility-date=2023-04-15",
    "swagger:generate": "npx swagger-typescript-api -p ./shared/client.json --modular -o ./shared/client/",
    "icons:generate": "npx @svgr/cli --out-dir app/components/common/Icons -- public/images/icons",
    "icons:watch": "npm-watch icons",
    "icons:svg": "run-s icons icons:watch",
    "postinstall": "remix-esbuild-override",
    "prettier": "prettier \"**/*.{js,jsx,ts,tsx,json,md,mdx,html,css}\"",
    "prettier:fix": "npm run prettier -- --write",
    "prettier:check": "npm run prettier -- --check",
    "eslint": "eslint . --ignore-path .gitignore",
    "eslint:fix": "eslint . --fix --ignore-path .gitignore",
    "lint:fix": "npm run prettier:fix && npm run eslint:fix",
    "lint": "npm run prettier:check && npm run eslint"
  },
  "eslintIgnore": [
    "/",
    "/build",
    "/public/build",
    "/shared"
  ],
  "dependencies": {
    "@remix-run/cloudflare": "^1.14.3",
    "@remix-run/cloudflare-pages": "^1.15.0",
    "@remix-run/react": "^1.15.0",
    "cross-env": "^7.0.3",
    "csstype": "^3.1.2",
    "date-fns": "^2.29.3",
    "framer-motion": "^10.12.4",
    "i18next": "^21.10.0",
    "i18next-browser-languagedetector": "^6.1.8",
    "i18next-fs-backend": "^1.2.0",
    "i18next-http-backend": "^1.4.5",
    "isbot": "^3.6.10",
    "qs": "^6.11.1",
    "rc-dropdown": "^4.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^11.18.6",
    "remix-i18next": "^4.2.0",
    "swiper": "^9.2.4",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.19.0",
    "@remix-run/dev": "^1.15.0",
    "@remix-run/eslint-config": "^1.15.0",
    "@svgr/cli": "^6.5.1",
    "@svgr/plugin-jsx": "^6.5.1",
    "@svgr/plugin-prettier": "^6.5.1",
    "@svgr/plugin-svgo": "^6.5.1",
    "@types/i18next-fs-backend": "1.1.2",
    "@types/qs": "^6.9.7",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.1",
    "@types/styled-components": "^5.1.26",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "babel-eslint": "^10.1.0",
    "eslint": "^8.39.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-no-cyrillic-string": "^1.0.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.1",
    "npm-run-all": "^4.1.5",
    "npm-watch": "^0.11.0",
    "nswag": "^13.18.2",
    "prettier": "2.8.5",
    "remix-esbuild-override": "^3.0.4",
    "styled-components": "^5.3.10",
    "tailwindcss": "^3.3.1",
    "typescript": "^4.9.5",
    "wrangler": "^2.17.0"
  },
  "engines": {
    "node": ">=16.13"
  },
  "watch": {
    "icons": {
      "patterns": [
        "icons"
      ],
      "extensions": "svg",
      "quiet": false
    }
  }
}
